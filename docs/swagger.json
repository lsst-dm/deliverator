{
    "swagger": "2.0",
    "info": {
        "title": "s3nd|The Deliverator API",
        "contact": {
            "name": "Support",
            "url": "https://github.com/lsst-dm/s3nd"
        },
        "license": {
            "name": "GPL-3.0",
            "url": "https://www.gnu.org/licenses/gpl-3.0.en.html"
        },
        "version": "0.0.0"
    },
    "paths": {
        "/upload": {
            "post": {
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "uploads"
                ],
                "summary": "upload file to S3",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Destination S3 URI",
                        "name": "uri",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "path to file to upload",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/requestStatus200"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/requestStatus400"
                        },
                        "headers": {
                            "X-Error": {
                                "type": "string",
                                "description": "error message"
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/requestStatus500"
                        },
                        "headers": {
                            "X-Error": {
                                "type": "string",
                                "description": "error message"
                            }
                        }
                    },
                    "504": {
                        "description": "Gateway Timeout",
                        "schema": {
                            "$ref": "#/definitions/requestStatus504"
                        },
                        "headers": {
                            "X-Error": {
                                "type": "string",
                                "description": "error message"
                            }
                        }
                    }
                }
            }
        },
        "/version": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "version"
                ],
                "summary": "report service version and configuration",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/VersionInfo"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "VersionInfo": {
            "type": "object",
            "properties": {
                "config": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "version": {
                    "type": "string",
                    "example": "0.0.0"
                }
            }
        },
        "requestStatus200": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "msg": {
                    "type": "string",
                    "example": "upload succeeded"
                },
                "task": {
                    "$ref": "#/definitions/task"
                }
            }
        },
        "requestStatus400": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 400
                },
                "msg": {
                    "type": "string",
                    "example": "error parsing request: missing field: uri"
                },
                "task": {
                    "type": "object",
                    "properties": {
                        "duration": {
                            "type": "string",
                            "example": "37.921µs"
                        },
                        "file": {
                            "type": "string",
                            "example": "/path/to/file.txt"
                        },
                        "id": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                }
            }
        },
        "requestStatus500": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 500
                },
                "msg": {
                    "type": "string",
                    "example": "upload attempt 5/5 timeout: operation error S3: PutObject, context deadline exceeded"
                },
                "task": {
                    "type": "object",
                    "properties": {
                        "attempts": {
                            "type": "integer",
                            "example": 5
                        },
                        "duration": {
                            "type": "string",
                            "example": "37.921µs"
                        },
                        "file": {
                            "type": "string",
                            "example": "/path/to/file.txt"
                        },
                        "id": {
                            "type": "string",
                            "format": "uuid"
                        },
                        "size_bytes": {
                            "type": "integer",
                            "example": 1000
                        },
                        "transfer_rate": {
                            "type": "string",
                            "example": "1000B/s"
                        },
                        "upload_parts": {
                            "type": "integer",
                            "example": 1
                        },
                        "uri": {
                            "type": "string",
                            "example": "s3://my-bucket/my-key"
                        }
                    }
                }
            }
        },
        "requestStatus504": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 504
                },
                "msg": {
                    "type": "string",
                    "example": "upload queue timeout: context deadline exceeded"
                },
                "task": {
                    "type": "object",
                    "properties": {
                        "duration": {
                            "type": "string",
                            "example": "56.115µs"
                        },
                        "file": {
                            "type": "string",
                            "example": "/path/to/file.txt"
                        },
                        "id": {
                            "type": "string",
                            "format": "uuid"
                        },
                        "uri": {
                            "type": "string",
                            "example": "s3://my-bucket/my-key"
                        }
                    }
                }
            }
        },
        "task": {
            "type": "object",
            "properties": {
                "attempts": {
                    "type": "integer",
                    "example": 1
                },
                "duration": {
                    "type": "string",
                    "example": "21.916462ms"
                },
                "file": {
                    "type": "string",
                    "example": "/path/to/file.txt"
                },
                "id": {
                    "type": "string",
                    "format": "uuid"
                },
                "size_bytes": {
                    "type": "integer",
                    "example": 1000
                },
                "transfer_rate": {
                    "type": "string",
                    "example": "1000B/s"
                },
                "upload_parts": {
                    "type": "integer",
                    "example": 1
                },
                "uri": {
                    "type": "string",
                    "example": "s3://my-bucket/my-key"
                }
            }
        }
    }
}